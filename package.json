{
  "name": "sportsee",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite -c vite.config.ts",
    "build": "tsc && vite build -c vite.config.dist.ts",
    "preview": "vite preview -c vite.config.dist.ts",
    "ladle": "ladle serve",
    "lint:eslint": "eslint",
    "lint:typescript": "tsc",
    "ci:build": "docker build -f docker/ci.dockerfile -t zuruh/sportsee-ci .",
    "server:start": "docker run --platform=linux/amd64 --rm --name sportsee-server -p 3000:3000 -d zuruh/sportsee-server yarn start",
    "server:stop": "docker stop $(docker container list --filter name=sportsee-server --format '{{.ID}}') 0"
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.4",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0",
    "recharts": "^2.5.0",
    "tailwindcss": "^3.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@ladle/react": "^2.12.1",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/node": "^18.15.11",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.22",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.9.5",
    "vite": "^4.2.2",
    "vite-plugin-svgr": "^2.4.0"
  }
}
